                                android:layout_height="wrap_content"
                                android:layout_gravity="right|center_vertical"
                                android:layout_weight="7"
                                android:padding="5dp"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:textSize="16sp" />

                        </TableRow>
                    </TableLayout>

                    <LinearLayout
