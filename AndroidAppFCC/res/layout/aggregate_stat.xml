                    android:layout_marginLeft="8dp"
                    android:layout_marginRight="8dp"
                    android:background="@drawable/black_alpha"
                    android:orientation="vertical"
                    android:visibility="gone" >

                    <TableLayout
                        android:id="@+id/jitter_header"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" >

                        <TableRow
                            android:id="@+id/TableRow01"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" >

                            <ImageView
                                android:id="@+id/btn_jitter_toggle"
                                style="?android:attr/buttonStyleSmall"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:background="@drawable/btn_down"
                                android:contentDescription="@string/open_panel"
                                android:minHeight="16dp"
                                android:minWidth="16dp" />

                            <TextView
                                android:layout_width="100dp"
                                android:layout_height="wrap_content"
                                android:layout_gravity="left|center_vertical"
                                android:layout_weight="3"
                                android:padding="5dp"
                                android:text="@string/jitter_ms"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:textColor="#202020"
                                android:textSize="18sp"
                                android:width="140dp" />

                            <TextView
                                android:id="@+id/jitter_average"
                                style="@style/column"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="right|center_vertical"
                                android:layout_weight="7"
                                android:padding="5dp"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:textSize="16sp" />

                        </TableRow>
                    </TableLayout>

                    <LinearLayout
                        android:id="@+id/jitter_content"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:visibility="gone" >

                        <WebView
                            android:id="@+id/jitter_graph"
                            style="@style/graphs"
                            android:layout_width="match_parent"
                            android:layout_height="160dp"
                            android:layout_margin="5dp"
                            android:contentDescription="@string/graph" />

                        <TableLayout
                            android:id="@+id/agggregate_test5_grid"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="left" >

                        </TableLayout>

                        <TableLayout
                            android:id="@+id/jitter_grid_nav"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" >

                            <TableRow
                                android:id="@+id/TableRow07"
                                style="@style/row"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="0dp"
                                android:paddingLeft="0dp" >

                                <Button
                                    android:id="@+id/btn_jitter_grid_left"
                                    style="?android:attr/buttonStyleSmall"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:background="@drawable/btn_left"
                                    android:contentDescription="@string/previous_grid"
                                    android:height="40dp"
                                    android:textColor="#000000" />

                                <TextView
                                    android:id="@+id/jitter_pagenumber"
                                    style="@style/normal"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="10"
                                    android:gravity="center_horizontal"
                                    android:padding="5dp"
                                    android:text=""
                                    android:textAppearance="?android:attr/textAppearanceMedium" />

                                <Button
                                    android:id="@+id/btn_jitter_grid_right"
                                    style="?android:attr/buttonStyleSmall"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:background="@drawable/btn_right"
                                    android:contentDescription="@string/next_grid"
                                    android:height="40dp"
                                    android:textColor="#000000" />

                            </TableRow>
                        </TableLayout>
                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>
        </LinearLayout>

        <ImageView
            android:id="@+id/ImageView01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:contentDescription="@string/paper_clip"
            android:src="@drawable/clip_shadow_small" />

    </RelativeLayout>

</ScrollView>